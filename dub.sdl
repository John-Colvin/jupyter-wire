name "jupyter_wire"
description "Jupyter kernel written in D"
authors "Atila Neves"
copyright "Copyright Â© 2018, Atila Neves"
license "boost"
dependency "zmqd" version="~>1.1.1"
// Only using asdf to do JSON serialisation
dependency "asdf" version="~>0.2.2"
targetType "library"


configuration "default" {

}

configuration "unittest" {
    targetType "executable"
